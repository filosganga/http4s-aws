<Configuration status="WARN">
  <Appenders>
    <File name="File" fileName="target/unit-test.log">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </File>
  </Appenders>
  <Loggers>
    <Root level="INFO">
      <AppenderRef ref="File"/>
    </Root>
    <Logger name="com.github.fd4s.http4s.aws" level="DEBUG"/>
  </Loggers>
</Configuration>